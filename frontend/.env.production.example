# Environment variables for production deployment
# Copy this file to .env.production and update values

# Backend API URL (ALB or CloudFront distribution)
REACT_APP_API_URL=https://api.your-domain.com/api

# Environment identifier
REACT_APP_ENV=production

# Analytics and monitoring (if needed)
REACT_APP_GA_TRACKING_ID=

# Feature flags
REACT_APP_ENABLE_MOCK_SERVICES=false
REACT_APP_ENABLE_ANALYTICS=true

# App metadata
REACT_APP_VERSION=$npm_package_version
REACT_APP_BUILD_DATE=

# AWS Region (for client-side AWS SDK if needed)
REACT_APP_AWS_REGION=us-east-1

# Disable source maps in production for security
GENERATE_SOURCEMAP=false